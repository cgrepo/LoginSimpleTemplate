<div class="login">
  <div class="container">
    <div class="form">
			<h1>Login</h1>
			<%= form_for(:user, url: sessions_path) do |f| %>
      	<%= f.text_field :username, :placeholder => "Username" %>
      	<%= f.password_field :password, :placeholder => "Password" %>
      	<%= f.submit "Login", class: "btn-submit" %>
  			<p><%#=debug(params)%></p>
  			<p id="notice"><%= notice %></p>
  			<%#= session.inspect %>
			<% end %>
    </div>
  </div>
</div>